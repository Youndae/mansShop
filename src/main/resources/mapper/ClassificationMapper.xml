<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.shop.mapper.ClassificationMapper">

    <select id="findAllId" resultType="String">
        SELECT id
        FROM classification
    </select>

    <select id="findAllIdOrderByClassificationStep" resultType="Classification">
        SELECT *
        FROM classification
        ORDER BY classificationStep
    </select>
</mapper>